import java.util.*;

public class Main
{
  public static void main (String[]args)
  {
    Scanner scanner = new Scanner (System.in);
    int n = scanner.nextInt();
    int[][] a = new int[n][n];
    for (int i = 0; i < n; i++)
    {
	    for(int j = 0; j < n; j++)
	    {
	        a[i][j] = scanner.nextInt();
	    }
    }
    for(int i = 0; i < n; i++)
    {
        for(int j = 0; j < i; j++)
        {
            if(a[i][j] == 1)
            {
                System.out.println((j + 1) + " " + (i + 1));
            }
        }
    }
    scanner.close();
  }
}
